{% extends "templates/base.peb" %}

{% block content %}
{% if resultado.foiErro %}
<h1> {{ resultado.getMsg }}</h1>
{% else %}

<ul>
    {% for bebida in bebidas %}
    <li>{{bebida.nome}}, R${{bebida.valor}}</li>
    {% endfor %}
</ul>
{% endif %}
<button><a href="/">Voltar</a></button>

{% endblock %}